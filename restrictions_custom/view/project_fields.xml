<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="proj_fields" model="ir.ui.view" >
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='tag_ids']" position="after">
                    
                      <field name="proj_value"/>
                      <field name="Received_Client_Payment"/>
                      <field name="proj_loc"/>
                     
                      
                      
                 </xpath> 
                  
            </field>
</record>


<record id="custom_field_domain_stock_inherit" model="ir.ui.view">
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="attributes">
                      <attribute name="domain">[('qty_available','>',0)]</attribute>
                 </xpath>
                 


            </field>
        </record>



</odoo>
